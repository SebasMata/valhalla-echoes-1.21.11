{
	"schemaVersion": 1,
	"id": "valhalla-echoes",
	"version": "${version}",
	"name": "Valhalla Echoes",
	"description": "Just a small private project to learn how to code Minecraft Java mods using Fabric. This mod adds a new mob: Mystical Vikings. They have overpowered weapons and tools you should try to get by farming a lot of Mystical Vikings.",
	"authors": [
		"Mataflex"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/SebasMata/valhalla-echoes-1.21.11"
	},
	"license": "CC0-1.0",
	"icon": "assets/valhalla-echoes/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.mataflex.ValhallaEchoes"
		],
		"client": [
			"com.mataflex.ValhallaEchoesClient"
		]
	},
	"mixins": [
		"valhalla-echoes.mixins.json",
		{
			"config": "valhalla-echoes.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	}
}